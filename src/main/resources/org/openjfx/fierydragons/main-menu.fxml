<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.fierydragons.render.SceneController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Button contentDisplay="CENTER" onAction="#switchToSettingScene" text="Start Game">
      <font>
         <Font name="Papyrus" size="96.0" />
      </font></Button>

  <Label fx:id="welcomeText" />
   <Button fx:id="loadGameButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#loadGame" onMousePressed="#loadGame" prefHeight="169.0" prefWidth="595.0" text="Load Game">
      <font>
         <Font size="90.0" />
      </font>
   </Button>
</VBox>
